{
  "name": "vue-string-number-ref",
  "version": "1.0.1",
  "license" : "MIT",
  "description": "This is a Vue ref wrapper that converts full-width input into half-width numbers in the IME.",
  "type": "module",
  "files": ["dist"],
  "main": "./dist/strNumRef.umd.cjs",
  "module": "./dist/strNumRef.js",
  "exports": {
    ".": {
      "types": "./dist/stringNumberRef.d.ts",
      "import": "./dist/strNumRef.js",
      "require": "./dist/strNumRef.umd.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/babu-ch/vue-string-number"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "typescript": "~5.6.2",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.4",
    "vue-tsc": "^2.2.0"
  }
}
